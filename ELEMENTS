elements=[]
limit=int(input("enter limit for list:"))
for i in range(limit):
    val=int(input("enter value:"))
    elements.append(val)
    
for i in elements:
    print(i,end=' ')

def large(li):
		big=li[0]
		for i in range(1,len(li)):
				if li[i]>big:
					     big=li[i]
		return big

def print_list(li):
		for i in li:
				print(i,end=' ')

print("elements in the list are")
print_list(elements)

print()

print("the largest elements is =   ",large(elements))     
